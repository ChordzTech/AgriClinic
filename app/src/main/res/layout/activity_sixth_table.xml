<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".loginActivities.cards.SixthTableActivity">

  <ScrollView
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:background="@color/gray">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".loginActivities.cards.FirstCardActivity">


      <androidx.appcompat.widget.Toolbar
          android:id="@+id/tbPageTitle"
          android:layout_width="match_parent"
          android:layout_height="?actionBarSize"
          android:background="@color/white"
          android:elevation="4dp"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/tvPageTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="रासायनिक खतांसाठी लागणारा गणिती ताळा"
            android:textColor="@color/black"
            android:textSize="18sp"
            android:textStyle="bold" />
      </androidx.appcompat.widget.Toolbar>

      <androidx.cardview.widget.CardView
          android:id="@+id/cvTableName2"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_margin="@dimen/margin_medium"
          app:cardCornerRadius="8dp"
          app:cardElevation="20dp"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/tbPageTitle">

        <TextView
            style="@style/ACTextMediumBold"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:padding="@dimen/padding_small"
            android:text=" प्रकार 2 -  सुरु ऊस / खोडवा लागवडीसाठी लागणारी रासायनीक खते (यूरिया + 10:26:26 ग्रेड )"
            />
      </androidx.cardview.widget.CardView>


      <androidx.cardview.widget.CardView
          android:id="@+id/cvEkakAndNPK"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_margin="@dimen/margin_medium"
          app:cardCornerRadius="8dp"
          app:cardElevation="20dp"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/cvTableName2">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

          <LinearLayout
              android:id="@+id/lltext"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="horizontal"
              android:weightSum="2"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toTopOf="parent">


            <TextView
                android:id="@+id/tvEkak"
                style="@style/ACTextMediumBold"
                android:layout_width="111dp"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:gravity="left"
                android:padding="@dimen/margin_small"
                android:text="क्षेत्र" />

            <TextView
                style="@style/ACTextMediumBold"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:gravity="left"
                android:padding="@dimen/margin_small"
                android:text="अन्नद्रव्याचे प्रमाण" />
          </LinearLayout>

          <LinearLayout
              android:id="@+id/linearLayout5"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_margin="@dimen/margin_small"
              android:orientation="vertical"
              app:layout_constraintBottom_toBottomOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@+id/lltext">

            <RadioGroup
                android:id="@+id/rgRadioGroup"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

              <RadioButton
                  android:id="@+id/rbEkar"
                  style="@style/ACTextMediumBold"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="एकर"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintTop_toBottomOf="@+id/tvEkak" />
              <View
                  android:layout_width="1dp"
                  android:layout_height="match_parent"
                  android:background="@color/black"/>

              <RadioButton
                  android:id="@+id/rbHector"
                  style="@style/ACTextMediumBold"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="हेक्टर"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintTop_toBottomOf="@+id/rbOption1" />
              <View
                  android:layout_width="1dp"
                  android:layout_height="match_parent"
                  android:background="@color/black"/>

              <RadioButton
                  android:id="@+id/rbGunta"
                  style="@style/ACTextMediumBold"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="गुंठा"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintTop_toBottomOf="@+id/rbOption2" />

            </RadioGroup>

          </LinearLayout>

          <LinearLayout
              android:id="@+id/linearLayout2"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:orientation="vertical"
              app:layout_constraintBottom_toBottomOf="parent"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toEndOf="@+id/linearLayout5"
              app:layout_constraintTop_toBottomOf="@+id/lltext">

            <TextView
                android:id="@+id/tvN"
                style="@style/ACTextMediumBold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="नत्र     - (N) :0" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>

            <TextView
                android:id="@+id/tvP"
                style="@style/ACTextMediumBold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_small"
                android:text="स्फुरद - (P) : 0" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>

            <TextView
                android:id="@+id/tvK"
                style="@style/ACTextMediumBold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_small"
                android:text="पालाश - (K) : 0" />

          </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
      </androidx.cardview.widget.CardView>

      <androidx.cardview.widget.CardView
          android:id="@+id/cvLandInput"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_margin="@dimen/margin_medium"
          app:cardCornerRadius="8dp"
          app:cardElevation="20dp"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/cvEkakAndNPK">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

          <TextView
              android:id="@+id/tvLandMeasurment"
              style="@style/ACTextMediumBold"
              android:layout_width="0dp"
              android:layout_height="wrap_content"
              android:gravity="center"
              android:padding="@dimen/padding_small"
              android:text="उसाच्या क्षेत्राचे मोज माप "
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toTopOf="parent" />

          <LinearLayout
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="horizontal"
              android:padding="@dimen/padding_small"
              android:weightSum="3"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@id/tvLandMeasurment">

            <Button
                android:id="@+id/btMinus"
                style="@style/ACTextMediumBold"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginStart="@dimen/margin_small"
                android:layout_weight="1"
                android:background="@drawable/circular_button"
                android:textColor="@color/black"
                android:text="-"
                android:textSize="32dp"/>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="100dp"
                android:layout_weight="1">

              <EditText
                  android:id="@+id/etLand"
                  style="@style/ACTextLargeBold"
                  android:layout_width="match_parent"
                  android:layout_height="0dp"
                  android:layout_weight="1"
                  android:gravity="center"
                  android:inputType="numberDecimal"
                  android:text="0.0"
                  app:layout_constraintEnd_toEndOf="parent"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintTop_toTopOf="parent" />

              <TextView
                  android:id="@+id/tvMeasurement"
                  style="@style/ACTextMediumBold"
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:gravity="center"
                  app:layout_constraintBottom_toBottomOf="parent"
                  app:layout_constraintEnd_toEndOf="parent"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintTop_toBottomOf="@+id/etLand" />
            </androidx.constraintlayout.widget.ConstraintLayout>


            <Button
                android:id="@+id/btPlus"
                style="@style/ACTextMediumBold"
                android:layout_width="0dp"
                android:layout_height="100dp"
                android:layout_marginEnd="@dimen/margin_small"
                android:layout_weight="1"
                android:background="@drawable/circular_button"
                android:backgroundTint="@null"
                android:gravity="center"
                android:text="+"
                android:textSize="32dp"/>

          </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

      </androidx.cardview.widget.CardView>

      <androidx.cardview.widget.CardView
          android:id="@+id/cvNPKCalcuator"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_margin="@dimen/margin_medium"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/cvLandInput">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


          <LinearLayout
              android:id="@+id/llNames"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="horizontal"
              android:weightSum="2"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toTopOf="parent">

            <TextView
                style="@style/ACTextMediumBold"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="यूरीया" />

            <TextView
                style="@style/ACTextMediumBold"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="10:26:26" />

          </LinearLayout>

          <LinearLayout
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="horizontal"
              android:weightSum="2"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@+id/llNames">

            <TextView
                android:id="@+id/tvUrea"
                style="@style/ACTextMediumBold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="0 किलो " />

            <TextView
                android:id="@+id/tvSsf"
                style="@style/ACTextMediumBold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="0 किलो" />


          </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
      </androidx.cardview.widget.CardView>

      <androidx.cardview.widget.CardView
          android:id="@+id/cvFertiTime"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_margin="@dimen/margin_medium"
          app:cardCornerRadius="8dp"
          app:cardElevation="20dp"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/cvNPKCalcuator">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="@+id/tvFirstStage"
            app:layout_constraintTop_toBottomOf="@+id/tvFirstStage">

          <TextView
              android:id="@+id/tvFirstStage"
              style="@style/ACTextMediumBold"
              android:layout_width="0dp"
              android:layout_height="wrap_content"
              android:gravity="center"
              android:text="पहिला हफ्ता"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toTopOf="parent" />

          <TextView
              android:id="@+id/tvBesalDose"
              style="@style/ACTextMediumBold"
              android:layout_width="0dp"
              android:layout_height="wrap_content"
              android:gravity="center"
              android:text="उस लागण करताना बेसल डोस"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@+id/tvFirstStage" />

          <LinearLayout
              android:id="@+id/llUSMTitle"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="horizontal"
              android:weightSum="3"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@+id/tvBesalDose">

            <TextView
                style="@style/ACTextMedium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="यूरीया" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>
            <TextView
                android:text="00 किलो"
                android:id="@+id/tvUreaInKg"
                style="@style/ACTextMedium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>
            <TextView
                android:id="@+id/tvUreaInBags"
                style="@style/ACTextMedium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="00 गोणी" />
          </LinearLayout>

          <LinearLayout
              android:id="@+id/llUSMInKg"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="horizontal"
              android:weightSum="3"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@+id/llUSMTitle">

            <TextView
                style="@style/ACTextMedium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="10:26:26" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>
            <TextView
                android:text="00 किलो"
                android:id="@+id/tvSSPInKg"
                style="@style/ACTextMedium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>
            <TextView
                android:id="@+id/tvSSPInBag"
                style="@style/ACTextMedium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="00 गोणी" />
          </LinearLayout>

          <LinearLayout
              android:id="@+id/llUSMInBag"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="horizontal"
              android:weightSum="3"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@+id/llUSMInKg">

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>
          </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
      </androidx.cardview.widget.CardView>


      <androidx.cardview.widget.CardView
          android:id="@+id/cvSecondWeek"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_margin="@dimen/margin_medium"
          app:cardCornerRadius="8dp"
          app:cardElevation="20dp"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/cvFertiTime">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="@+id/tvFirstStage"
            app:layout_constraintTop_toBottomOf="@+id/tvFirstStage">

          <TextView
              android:id="@+id/tvSecondStage"
              style="@style/ACTextMediumBold"
              android:layout_width="0dp"
              android:layout_height="wrap_content"
              android:gravity="center"
              android:text="दुसरा हफ्ता"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toTopOf="parent" />

          <TextView
              android:id="@+id/tvSecondWeekText"
              style="@style/ACTextMediumBold"
              android:layout_width="0dp"
              android:layout_height="wrap_content"
              android:gravity="center"
              android:text="उस लागवडीनंतर दीड ते दोन महिन्यांनी"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@+id/tvSecondStage" />

          <LinearLayout
              android:id="@+id/llUSMTitleSecondWeek"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="horizontal"
              android:weightSum="3"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@+id/tvSecondWeekText">

            <TextView
                style="@style/ACTextMedium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="युरिया" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>
            <TextView
                android:id="@+id/ureaInKg"
                style="@style/ACTextMedium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="00 किलो" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>
            <TextView
                android:id="@+id/Urea"
                style="@style/ACTextMedium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="00 गोणी " />
             </LinearLayout>

          <LinearLayout
              android:id="@+id/llUSMInKgSecondWeek"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="horizontal"
              android:weightSum="3"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@+id/llUSMTitleSecondWeek">

            <TextView
                style="@style/ACTextMedium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="10:26:26" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>
            <TextView
                android:id="@+id/SSPInKg"
                style="@style/ACTextMedium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="00 किलो" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>
            <TextView
                android:id="@+id/SSF"
                style="@style/ACTextMedium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/padding_small"
                android:text="00 गोणी" />
          </LinearLayout>

          <LinearLayout
              android:id="@+id/llUSMInBagSecondWeek"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="horizontal"
              android:weightSum="3"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@+id/llUSMInKgSecondWeek">

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/black"/>
          </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
      </androidx.cardview.widget.CardView>

      <androidx.cardview.widget.CardView
          android:id="@+id/cvThirdWeek"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_margin="@dimen/margin_medium"
          app:cardCornerRadius="8dp"
          app:cardElevation="20dp"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toBottomOf="@+id/cvSecondWeek">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="@+id/tvFirstStage"
            app:layout_constraintTop_toBottomOf="@+id/tvFirstStage">

